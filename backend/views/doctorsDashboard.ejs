                                        </div>
                                        <div class="col-auto">
                                            <i class="fas fa-comments fa-2x text-gray-300"></i>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <!-- Content Row -->

                    <div class="row">

                        <!-- Area Chart -->
                        <div class="col-xl-8 col-lg-7">
                            <div class="card shadow mb-4">
                              
                                <div
                                    class="card-header py-3 d-flex flex-row align-items-center justify-content-between">
                                    <h6 class="m-0 font-weight-bold text-primary">Patients Details</h6>
                                    <div class="dropdown no-arrow">
                                        <a class="dropdown-toggle" href="#" role="button" id="dropdownMenuLink"
                                            data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                            <i class="fas fa-ellipsis-v fa-sm fa-fw text-gray-400"></i>
                                        </a>
                                        <div class="dropdown-menu dropdown-menu-right shadow animated--fade-in"
                                            aria-labelledby="dropdownMenuLink">
                                            <div class="dropdown-header">Action:</div>
                                            <a class="dropdown-item" href="#">View Completed</a>
                                            <a class="dropdown-item" href="#">Add recording</a>
                                            <div class="dropdown-divider"></div>
                                            <a class="dropdown-item" href="#">Patient at Risk</a>
                                        </div>
                                    </div>
                                </div>
                                <div class="card-body">
                                    <table class="table table-striped">
                                        <thead>
                                          <tr>
                                            <th scope="col">#</th>
                                            <th scope="col">Name</th>
                                            <th scope="col">Status</th>
                                            <th scope="col">Open</th>
                                          </tr>
                                        </thead>
                                        <tbody>
                                        <!-- foreach loop ejs -->
                                        <% for (var i = 0; i < patients.length; i++) { %>
                                            <tr>
                                                <th scope="row"><%= patients[i].patientId %></th>
                                                <td><%= patients[i].name %></td>
                                                <td><%= patients[i].status %></td>
                                                <td><a href="/patient/<%= patients[i]._id %>">Open</a></td>
                                            </tr>
                                        <% } %>
                                          
                                        </tbody>
                                      </table>
                                      <!-- <table id="pat" width="100%" border= "0">
                                
                                        <tr>
                                            
                                         <td class="tab"><div class="text-xs font-weight-bold text-warning text-uppercase mb-1">ID</div></td>
                                         
                                         <td class="tab"><div class="text-xs font-weight-bold text-warning text-uppercase mb-1">NAME</div></td>
                                         <td class="tab"><div class="text-xs font-weight-bold text-warning text-uppercase mb-1">STATUS</div></td>
                                      </tr>
                                  </table> -->
                                </div>
                            </div>
                        </div>
                        </div>
